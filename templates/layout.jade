      link(rel="icon", type="image/png", href="/images/favicon.png")
      script(src='/js/highlight.pack.js')
      script(src='/js/jquery.min.js')
      script(src='/js/bootstrap.min.js')
      script(src='/js/underscore.js')
      script(src='/js/global.js')
      block addlocaljs
